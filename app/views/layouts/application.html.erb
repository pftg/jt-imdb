<!DOCTYPE html>
<html>
<head>
  <title>Imdb</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
<%= render 'layouts/head' %>
<div data-behavior="notices">
  <%= render 'layouts/notices', flash: flash %>
</div>

<div class="container">
  <%= yield %>
</div>
</body>
</html>
